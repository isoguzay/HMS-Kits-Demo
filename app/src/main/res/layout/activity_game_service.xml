<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.GameServiceActivity">

    <Button
        android:id="@+id/button_game_init"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:text="init"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.097" />

    <Button
        android:id="@+id/button_game_get_current_player"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:text="Get Current Player"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_game_sign_in"
        app:layout_constraintVertical_bias="0.073" />

    <Button
        android:id="@+id/button_game_submit_begin"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:text="Submit Player Event Begin"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_game_get_current_player"
        app:layout_constraintVertical_bias="0.09" />

    <Button
        android:id="@+id/button_game_get_player_info"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:text="Get Player Extra Info"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_game_submit_end"
        app:layout_constraintVertical_bias="0.171" />

    <Button
        android:id="@+id/button_game_submit_end"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:text="Submit Player Event End"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_game_submit_begin"
        app:layout_constraintVertical_bias="0.131" />

    <Button
        android:id="@+id/button_game_sign_in"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:text="Sign In"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_game_init"
        app:layout_constraintVertical_bias="0.056" />

</androidx.constraintlayout.widget.ConstraintLayout>